{
  "expo": {
    "name": "TripBee",
    "slug": "tripbee-nicolestella",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tripbee"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This is TripBee, it is an app that allows users to browse through popular travel destinations and save them on their profile like a wishlist. Users can also add their own custom listing and edit it."
  }
}
